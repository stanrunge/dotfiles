{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "aws.resources.enabledResources": ["AWS::Route53::HostedZone"],
  "aws.suppressPrompts": {
    "fileViewerEdit": true,
    "regionAddAutomatically": true
  },
  "bootstrapIntelliSense.version": "Bootstrap v5.3",
  "cloudcode.project": "vash-esports",
  "editor.accessibilitySupport": "off",
  "editor.fontFamily": "JetBrainsMono Nerd Font, Geist Mono, Jetbrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "editor.fontWeight": "bold",
  "editor.hover.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.rulers": [80, 120],
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "css": false,
    "java": true,
    "javascript": true,
    "yaml": true
  },
  "githubPullRequests.fileListLayout": "tree",
  "githubPullRequests.pullBranch": "never",
