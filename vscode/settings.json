    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "vs-kubernetes": {
    "vscode-kubernetes.kubectl-path-mac": "/Users/stanrunge/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.helm-path-mac": "/Users/stanrunge/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path-mac": "/Users/stanrunge/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "is:open assignee:${user} repo:${owner}/${repository}",
      "groupBy": ["milestone"]
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "Recent Issues",
      "query": "state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "gitlens.ai.experimental.model": "openai:gpt-4o",
  "terminal.integrated.enableImages": true,
  "window.restoreFullscreen": true,
  "workbench.editor.showTabs": "none",
  "dart.flutterSdkPath": "/Users/stanrunge/Development/flutter",
  "makefile.configureOnOpen": true,
  "vim.easymotion": true,
  "lldb.suppressUpdateNotifications": true,
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
